{
  //https://api.cdnjs.com/libraries?output=human
  //https://api.cdnjs.com/libraries/jquery?output=human
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/lib",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery",
      "files": [ "jquery.js", "jquery.min.js", "jquery.min.map" ]
    },
    {
      "library": "jquery-validate@1.20.0",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [ "jquery.validate.js", "jquery.validate.min.js", "additional-methods.js", "additional-methods.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [
        "jquery.validate.unobtrusive.js",
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jQuery-linkify@4.1.3",
      "destination": "wwwroot/lib/jQuery-linkify",
      "files": [
        "linkify.js",
        "linkify.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "font-awesome@6.4.2",
      "destination": "wwwroot/lib/font-awesome",
      "files": [
        "js/all.js",
        "js/all.min.js",
        "css/all.css",
        "css/all.min.css",
        "sprites/brands.svg",
        "sprites/regular.svg",
        "sprites/solid.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2",
      "files": [
        "css/select2.css",
        "css/select2.min.css",
        "js/select2.js",
        "js/select2.min.js"
      ]
    },
    {
      "library": "handlebars.js@4.7.8",
      "destination": "wwwroot/lib/handlebars",
      "files": [
        "handlebars.js",
        "handlebars.min.js"
      ]
    }
  ]
}